{
	"name": "gitpod-ams",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check --ignore-path=../../.prettierignore . && eslint .",
		"format": "prettier --plugin-search-dir . --write --ignore-path=../../.prettierignore ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"typescript": "^5.0.2",
		"vite": "^4.2.0",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"svelte-preprocess": "^5.0.3",
		"autoprefixer": "^10.4.7",
		"tailwindcss": "^3.1.5"
	},
	"type": "module"
}
